[user]
	email = "test@example.com"
	name = "test"

[push]
	default = current

[pull]
	rebase = true

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true
    light = false
    side-by-side = true
    line-numbers = true

[alias]
    tree = log --graph --all --decorate --oneline
